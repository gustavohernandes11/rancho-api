{
	"compilerOptions": {
		"strictNullChecks": true,
		"module": "commonjs",
		"target": "es2019",
		"moduleResolution": "node",
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"baseUrl": "src",
		"paths": {
			"@/*": ["*"]
		}
	}
}
